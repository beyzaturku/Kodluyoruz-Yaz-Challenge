class Hard {
  static void Main() {
    Console.WriteLine("Bir cumle giriniz:");
    string cumle = Console.ReadLine();
    
    string[] kelime = cumle.Split(' ');
    
    int sayac =0;
        
    for(int i=0; i<kelime.Length-1; i++){
        if(kelime[i] == kelime[i+1]){
            sayac++; 
        }
    }
    
    if(sayac > 0){
        Console.WriteLine("Ikilime kullandiniz");
    }
    else{
        Console.WriteLine("Ikilime kullanmadiniz.");
    }
      

  }
}
