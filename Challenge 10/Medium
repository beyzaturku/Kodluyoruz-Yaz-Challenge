class Medium {
  static void Main() {
    int[] dizi = {1,7,9,15,14};
    
    int enBuyuk = dizi[0];
    int enKucuk = dizi[0];
    
    for(int i=0; i<dizi.Length; i++){
        if(dizi[i] > enBuyuk){
            enBuyuk = dizi[i];
        }
        else if(dizi[i] < enKucuk){
            enKucuk = dizi[i];
        }
    }
    
   
    
    Console.WriteLine($"En Büyük Sayı: {enBuyuk}");
    Console.WriteLine($"En Kücük Sayi: {enKucuk}");
  }
}
